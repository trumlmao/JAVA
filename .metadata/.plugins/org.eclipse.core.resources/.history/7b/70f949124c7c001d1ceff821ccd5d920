package GUI;

import javax.swing.JFrame;

public class TEST2 extends JFrame {
	public TEST2() {
		
	}
	public void show1() {
		this.setVisible(true);
		
	}
	
	public void show1(String title) {
		this.setTitle(title);
		this.setVisible(true);
	}
	public void show1(String title,int width,int height) {
		this.setTitle(title);
		this.setSize(width,height);
		this.setVisible(true);
	}
	public static void main(String[] args) {
		TEST2 fr = new TEST2();
		fr.show1("FR1");
		
		TEST2 fr2 = new TEST2();
		
		fr2.show1("FR2");
		
	}
}
